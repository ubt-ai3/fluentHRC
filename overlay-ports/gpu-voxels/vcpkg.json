{
  "name": "gpu-voxels",
  "version": "0.6.4",
  "port-version": 1,
  "description": "GPU-Voxels is a CUDA based library which allows high resolution volumetric collision detection between animated 3D models and live pointclouds from 3D sensors of all kinds.",
  "homepage": "https://github.com/Olli1080/gpu-voxels",
  "license": null,
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    { 
      "name": "pcl",
      "features": ["openni2"]
    },
    "boost-interprocess",
    "boost-test",
    "tinyxml2",
    "cppunit",
    "glew",
    "glm",
    "freeglut",
    "zlib",
    "stb",
    "eigen3"
  ]
}
